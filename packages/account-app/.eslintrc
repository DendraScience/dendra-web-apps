{
    "extends":
    [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:vue/vue3-recommended",
        "prettier"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions":
    {
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins":
    [
        "@typescript-eslint"
    ],
    "rules":
    {
        "vue/multi-word-component-names":
        [
            "error",
            {
                "ignores":
                [
                    "+Page"
                ]
            }
        ]
    }
}